{
    "routes": [
        {
            "src": "/img/(.*)",
            "headers": { "cache-control": "max-age=604800" },
            "dest": "/img/$1"
        },
        {
            "src": "/styles/(.*)",
            "headers": { "cache-control": "max-age=604800" },
            "dest": "/styles/$1"
        }
    ]
}
